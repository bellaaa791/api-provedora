const axios = require('axios')
const cheerio = require('cheerio')
async function XGroovyPhotos() {

    const baseUrl = 'https://xgroovy.com/photos/categories/hentai/';
    const randomPage = Math.floor(Math.random() * 695);
    const url = `${baseUrl}${randomPage}`;

    try {
        const { data } = await axios.get(url, {
            headers: {
                'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/114.0.0.0 Safari/537.36'
            }
        });

        const $ = cheerio.load(data);
        const results = [];

        $('img.thumb').each((_, element) => {
            const imageUrl = $(element).attr('src');
            const title = $(element).attr('alt');

            if (imageUrl && title) {
                results.push({ imageUrl, });
            }
        });

        return results;
    } catch (error) {
        console.error('Error fetching XGroovy Hentai:', error.message);
        return [];
    }
}


module.exports = { XGroovyPhotos }